[general]
# You HAVE to add the rule here to enable it, only configuring (such as below)
# does NOT enable it.
contrib=contrib-title-conventional-commits

[ignore-by-author-name]
regex=^Renovate Bot$
ignore=body-is-missing,title-max-length

[ignore-by-title]
regex=^Merge [[:xdigit:]]{40} into [[:xdigit:]]{40}$
ignore=CT1,T1,B6
